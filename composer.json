{
  "name": "alina-yavd/serializer",
  "description": "PHP serialize library.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "pear/xml_serializer": "^0.22.0",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "AYS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AYS\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
